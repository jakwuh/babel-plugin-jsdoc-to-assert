{
  "name": "babel-plugin-jsdoc-to-condition",
  "version": "0.3.1",
  "description": "A babel plugin which creates validation code from jsdoc comments",
  "main": "lib/index.js",
  "repository": "git@github.com:jakwuh/babel-plugin-jsdoc-to-condition.git",
  "author": "jakwuh <jakwuh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run build && npm test",
    "test": "ava",
    "start": "npm run watch | ava -w"
  },
  "keywords": [
    "babel",
    "plugin",
    "assert",
    "validation",
    "testing"
  ],
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "es6-promisify": "^5.0.0",
    "flow-bin": "^0.54.0"
  },
  "dependencies": {
    "doctrine": "^2.0.0",
    "escape-regex-string": "^1.0.4",
    "jsdoc-to-condition": "^0.3.0",
    "lodash": "3||4"
  },
  "ava": {
    "files": [
      "./tests/index.js"
    ],
    "source": [
      "./src/*.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
